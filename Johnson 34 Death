		)
		espsettings:Label("Default Key 't' ^", Color3.fromRGB(38, 38, 38), Color3.fromRGB(0, 216, 111))

		Mouse.KeyDown:Connect(
			function(TheKeyPressed)
				if TheKeyPressed == (string.lower(EspToggleBind) or string.upper(EspToggleBind)) then
					if allesp == true then
						allesp = false
					else
						allesp = true
					end
				end
			end
		)

