		local FileName = "Ai Assist.txt"
		local HttpService = game:GetService("HttpService")
		local SaveSettings = function(Text)
			writefile(FileName, HttpService:JSONEncode(Text))
		end
		local SavedSettings
		if isfile(FileName) then
			SavedSettings = HttpService:JSONDecode(readfile(FileName))
		else
			warn("Settings File Not Found!")
		end

		hbee:Button(
			"Hbe",
			function()
				hbeexecuted = true
				_G.FirstTime = true
				spawn(
					function()
						local TORSOSIZEEEE =
							Vector3.new(
								game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Size.X,
								game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Size.Y,
								game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Size.Z
							)
						local gmt = getrawmetatable(game)
						local oldindex = gmt.__index

						if setreadonly then
							setreadonly(gmt, false)
						else
							make_writeable(gmt, true)
						end

						gmt.__index =
							newcclosure(
								function(t, k)
									if type(t) == "userdata" and oldindex(t, "Name") == "HumanoidRootPart" and k == "Size" then
									return TORSOSIZEEEE
								end
									return oldindex(t, k)
								end
							)

						if setreadonly then
							setreadonly(gmt, true)
						else
							make_writeable(gmt, false)
						end
						wait(2)
						Updateeeeee()
					end
				)
			end
		)

		hbee:Button(
			"Save Settings!",
			function()
				if hbeexecuted == true then
					local Configuraciones = {
						hbeeX = hbeeX,
						hbeeY = hbeeY,
						hbeeZ = hbeeZ,
						hbeeOn = hbeeOn,
						hbeexecuted = hbeexecuted
					}
					SaveSettings(Configuraciones)
				else
					local Configuraciones = {
						hbeeX = hbeeX,
						hbeeY = hbeeY,
						hbeeZ = hbeeZ,
						hbeeOn = hbeeOn
					}
					SaveSettings(Configuraciones)
				end
			end
		)

		hbee:Button(
			"Load Settings!",
			function()
				hbeeX = SavedSettings.hbeeX
				hbeeY = SavedSettings.hbeeY
				hbeeZ = SavedSettings.hbeeZ
				hbeeOn = SavedSettings.hbeeOn
				if SavedSettings.hbeexecuted == true then
					spawn(
						function()
							local TORSOSIZEEEE =
								Vector3.new(
									game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Size.X,
									game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Size.Y,
									game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Size.Z
								)
							local gmt = getrawmetatable(game)
							local oldindex = gmt.__index

							if setreadonly then
								setreadonly(gmt, false)
							else
								make_writeable(gmt, true)
							end

							gmt.__index =
								newcclosure(
									function(t, k)
										if
										type(t) == "userdata" and oldindex(t, "Name") == "HumanoidRootPart" and
										k == "Size"
									then
										return TORSOSIZEEEE
									end
										return oldindex(t, k)
									end
								)

							if setreadonly then
								setreadonly(gmt, true)
							else
								make_writeable(gmt, false)
							end
							wait(2)
							hbeexecuted = true
							Updateeeeee()
						end
					)
				end
			end
		)

