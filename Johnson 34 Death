		local ToRespawn = false
		local hbeexecuted = false
		local hbeeX = 4
		local hbeeY = 4
		local hbeeZ = 2
		local hbeeOn = false

		local Updateeeeee = function()
			if hbeexecuted == true then
				for i, v in pairs(game:GetDescendants()) do
					if
						v:IsA("Part") and v.Name == "HumanoidRootPart" and
						v.Parent.Name ~= game.Players.LocalPlayer.Name and
						v.Name ~= "" and
						v.Parent.Name ~= ""
					then
						if hbeeOn == true then
							v.Size = Vector3.new(2, 2, 1)
							v.Material = "ForceField"
							v.Transparency = 1
						else
							v.Size = Vector3.new(hbeeX, hbeeY, hbeeZ)
							v.Material = "ForceField"
							if _G.Recording == true then
								v.Transparency = 1
							else
								v.Transparency = 0
							end
						end
					end
				end
				if ToRespawn == false then
					ToRespawn = true
					game.DescendantAdded:Connect(
						function(v)
							if
								v:IsA("Part") and v.Name == "HumanoidRootPart" and
								v.Parent.Name ~= game.Players.LocalPlayer.Name and
								v.Name ~= "" and
								v.Parent.Name ~= ""
							then
								if hbeeOn == true then
									v.Size = Vector3.new(2, 2, 1)
									v.Material = "ForceField"
									v.Transparency = 1
								else
									v.Size = Vector3.new(hbeeX, hbeeY, hbeeZ)
									v.Material = "ForceField"
									if _G.Recording == true then
										v.Transparency = 1
									else
										v.Transparency = 0
									end
								end
							end
						end
					)
				end
			end
		end

		hbee:Toggle(
			"Hitbox Extender",
			function(bap)
				hbeeOn = not bap
				wait()
				Updateeeeee()
			end
		)

		hbee:Slider(
			"X Size",
			2,
			200,
