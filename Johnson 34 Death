        local t = object[prop]
    end
    
    function NotTeam(Player)
      	if Services.Players:FindFirstChild(Player) then
    		return Services.Players[Player].Team ~= Services.Players.LocalPlayer.Team and Services.Players[Player].TeamColor ~= Services.Players.LocalPlayer.TeamColor
      	end
      	
    	return true
    end
      
    local Patch_Phantom_Forces = function()
      	spawn(function()
            Client = {}
            for i,v in next,getgc(true) do
                if type(v) == "table" and rawget(v,"getbodyparts") then
                    Client.Characters = getupvalue(v.getbodyparts,1)
                    break
                end
            end
            while true do
                for i,v in next,Services.Players:GetPlayers() do
                    if v ~= Services.Players.LocalPlayer then
                        local Character = Client.Characters[v]
                        if Character then
                            Character.head.Parent.Name = v.Name
                            v.Character = Character.head.Parent
                        end
                    end
                end
                FastWait()
            end
        end)
    end
    
    if game.CreatorId == 1103278 then --Phantom Forces
        Patch_Phantom_Forces()
    end
    
    function EspPosition(Part)
        local Position = Services.Workspace.CurrentCamera:WorldToViewportPoint(Part.Position)
        return Vector2.new(Position.X - Part.Size.X / 2,Position.Y - Part.Size.Y / 2)
    end
    
    function EspPosition2(Part)
        local Position = Services.Workspace.CurrentCamera:WorldToViewportPoint(Part.Position + Vector3.new(0,0.5 + Part.Size.Y / 2,0))
    	
        return Vector2.new(Position.X - Part.Size.X / 2,Position.Y - Part.Size.Y / 2)
    end
    
    function WorldToViewportPoint(Position)
        return Services.Workspace.CurrentCamera:WorldToViewportPoint(Position)
    end
    
    if game.PlaceId == 379614936 or game.PlaceId == 5006801542 or game.CreatorId == 113657341 then -- Assassin
        function UpdateTargets()
            local TargetText =Services.Players.LocalPlayer:FindFirstChild("TargetText",true)
            if TargetText then
                Target = Services.Players:FindFirstChild(TargetText.Text)
            end
        end
    elseif game.PlaceId == 142823291 then -- Murder Mystery 2
        local GetPlayerData = Services.ReplicatedStorage.GetPlayerData
    
        function UpdateTargets()
            local Roles = GetPlayerData:InvokeServer()
    
            Target = nil
            Sheriff = nil
    
            for i,v in next,Roles do
                if v.Role == "Murderer" then
                    Target = Services.Players:FindFirstChild(i)
                elseif v.Role == "Hero" then
                    Sheriff = Services.Players:FindFirstChild(i)
                end
            end
    
            if not Sheriff then
                for i,v in next,Roles do
                    if v.Role == "Sheriff" then
                        Sheriff = Services.Players:FindFirstChild(i)
    
                        break
                    end
                end
            end
        end
    elseif game.PlaceId == 1167791961 then -- Knife Simulator
        function UpdateTargets()
            Target = nil
    
            for i,v in next,Services.Players:GetPlayers() do
                if v.Character and v.Character:FindFirstChild("crown") then
                    Target = v
    
                    break
                end
            end
        end
    elseif game.PlaceId == 621129760 then -- KAT!
        function UpdateTargets()
            local TargetName =Services.Players.LocalPlayer:FindFirstChild("TargetName",true)
            if TargetName then
                Target = Services.Players:FindFirstChild(TargetName.Text)
            end
        end
    elseif game.PlaceId == 893973440 then -- Flee the Facility
        function UpdateTargets()
            Target = nil
            for i,v in next,Services.Players:GetPlayers() do
                if v.Character and v.Character:FindFirstChild("Gemstone") then
                    Target = v
                    break
                end
            end
        end
    elseif game.PlaceId == 3233893879 then -- Bad Business
      	Client = require(Services.ReplicatedStorage.TS)
      	
    	function NotTeam(Player)
        	if Services.Players:FindFirstChild(Player) then
            	return Client.Teams:GetPlayerTeam(Services.Players[Player]) ~= Client.Teams:GetPlayerTeam(Services.Players.LocalPlayer)
          	end
        	return true
        end
    end
    
    Services.RunService.RenderStepped:Connect(function()
        if UpdateTargets then
            UpdateTargets()
        end
    end)
    
    local Heads,RootParts = {},{}
    spawn(function()
        for i,v in next,Services.Workspace:GetDescendants() do
            if i % 50 == 0 then FastWait() end
            for k,va in pairs(Known_Names) do
                if table.find(Classes,v.ClassName) and v.Parent and v.Parent.Name ~=Services.Players.LocalPlayer.Name and v.Parent.Name ~= "" and not Banned_Names[v.Parent.Name] and Known_Names.HumanoidRootPart[v.Name] and k == "Head" and v.Parent:FindFirstChild(va[k]) then
                    local success = pcall(function() hasProperty(v.Parent.Head, "Position") end)
                    if success then
                        table.insert(Heads,v.Parent.Head)
                    end
                end
            end
        end
    end)
    
    Services.Workspace.DescendantAdded:Connect(function(v)
        repeat
          wait()
        until v ~= nil;
        local egh;
        if v ~= nil then
        	egh = true
        else
          	egh = false
        end
        if egh then
            for k,va in pairs(Known_Names) do
                if egh and table.find(Classes,v.ClassName) and v.Parent and v.Parent.Name ~= Services.Players.LocalPlayer.Name and v.Parent.Name ~= "" and not Banned_Names[v.Parent.Name] and Known_Names.HumanoidRootPart[v.Name] and k == "Head" and v.Parent:FindFirstChild(va[k]) then
                    local success = pcall(function() hasProperty(v.Parent.Head, "Position") end)
                    if success then
                        table.insert(Heads,v.Parent.Head)
                    end
                end
            end
        end
    end)
    
    Services.Workspace.DescendantRemoving:Connect(function(v)
        if table.find(Heads,v) then
            table.remove(Heads,table.find(Heads,v))
        end
    end)
    
    spawn(function()
        for i,v in next,Services.Workspace:GetDescendants() do
            if i % 50 == 0 then FastWait() end
            for k,va in pairs(Known_Names) do
                if table.find(Classes,v.ClassName) and v.Parent and v.Parent.Name ~=Services.Players.LocalPlayer.Name and v.Parent.Name ~= "" and not Banned_Names[v.Parent.Name] and Known_Names.HumanoidRootPart[v.Name] and k == "Head" and v.Parent:FindFirstChild(va[k]) then
