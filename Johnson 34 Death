    spawn(function()
        local get_my_pos = function()
            pcall(function()
                if Services.Players.LocalPlayer.Head then
                    return Services.Players.LocalPlayer.Head
                else
                    return Services.Players.LocalPlayer.Head
                end
            end)
        end
    end)
    
    function Get_Closest_Root()
        local part;
        local shortestDistance = MAXDISTANCE * 2;
        for i,v in next,RootParts do
            local Head_x = v
            local pos = Services.Workspace.CurrentCamera:WorldToViewportPoint(Head_x.Position)
            local magnitude = (Vector2.new(pos.X, pos.Y) - Services.UserInputService:GetMouseLocation()).magnitude
        	if magnitude <= shortestDistance and Head_x.Parent then
          		local hm = Head_x.Parent:FindFirstChild("Humanoid") or nil;
          		if hm then
            		if hm.Health <= 0 then
              			shortestDistance = magnitude
                        part = Services.Players.LocalPlayer
            		else
                        local mags = Services.Players.LocalPlayer:DistanceFromCharacter(Head_x.Position)
                        if checkkFar == true and mags <= EspDistance or not checkkFar then
                            shortestDistance = magnitude
                            part = Head_x
                        end
            		end
            	else
            		local mags = Services.Players.LocalPlayer:DistanceFromCharacter(Head_x.Position)
                    if checkkFar == true and mags <= EspDistance or not checkkFar then
                        shortestDistance = magnitude
                        part = Head_x
                    end
          		end
            end
        end
    	return part;
    end
    
    function Get_Closest_Head()
        local part;
        local shortestDistance = MAXDISTANCE * 2;
        for i,v in next,Heads do
            local Head_x = v
            local pos = Services.Workspace.CurrentCamera:WorldToViewportPoint(Head_x.Position)
            local magnitude = (Vector2.new(pos.X, pos.Y) - Services.UserInputService:GetMouseLocation()).magnitude
            if magnitude <= shortestDistance and Head_x.Parent then
          		local hm = Head_x.Parent:FindFirstChild("Humanoid") or nil;
          		if hm then
            		if hm.Health <= 0 then
              			shortestDistance = magnitude
                        part = Services.Players.LocalPlayer
            		else
                        local mags = Services.Players.LocalPlayer:DistanceFromCharacter(Head_x.Position)
                        if checkkFar == true and mags <= EspDistance or not checkkFar then
                            shortestDistance = magnitude
                            part = Head_x
                        end
            		end
            	else
            		local mags = Services.Players.LocalPlayer:DistanceFromCharacter(Head_x.Position)
                    if checkkFar == true and mags <= EspDistance or not checkkFar then
                        shortestDistance = magnitude
                        part = Head_x
                    end
          		end
            end
        end
    	return part;
    end
    
    spawn(function()
        local Circle = Drawing.new("Circle")
        Circle.Visible = false
        Circle.Position = Vector2.new(mouse.X, mouse.Y)
        Circle.Color = Color3.fromRGB(220, 53, 53)
        Circle.Thickness = 0.001
        Circle.NumSides = 0
        Circle.Radius = MAXDISTANCE
        Circle.Filled = false
        Circle.Transparency = 0.50
        Rotating = 50000
        game:GetService("RunService").Heartbeat:Connect(function()
            if ButtonOn == true and showfovaimbot == true then
                pcall(function()
                    Circle.NumSides = Rotating
                    Circle.Position = Vector2.new(mouse.X, mouse.Y)
                    Circle.Radius = MAXDISTANCE
                    Circle.Color = Color3.fromHSV(tick() % 5 / 5,1,1)
                    Circle.Visible = true
                end)
            else
                pcall(function()
                    Circle.Position = Vector2.new(mouse.X, mouse.Y)
                    Circle.Radius = MAXDISTANCE
                    Circle.Visible = false
                end)
            end
        end)
    end)
    
