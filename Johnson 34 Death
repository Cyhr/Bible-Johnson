					)
				end
			end
		)

		spawn(
			function()
				local FASTTHROWWBind = "g"
				local ThrowKnifegay = function()
					if _G.EstoyTirando == true then
						for i, v in pairs(Player.Backpack:GetChildren()) do
							if v:IsA("Tool") and v.Name == "Knife" then
								_G.EstoyTirando = false
							end
						end
					else
						if CheckKnife() == true then
							_G.EstoyTirando = true
							for i = 1, 1 do
								for i, v in pairs(Player.Backpack:GetChildren()) do
									if v:IsA("Tool") and v.Name == "Knife" then
										Player.Character.Humanoid:EquipTool(v)
									end
								end
								if game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife then
									local ElValorDelRotador =
										game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.RotateValue.Value
									if
										game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
										"throwOrientation"
										)
									then
										ElValorDelRotador =
											game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
										"throwOrientation"
										).Value
									end
									wait()
									Update()
									wait()
									_G.aimboton = true
									local Anim =
										game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
											game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.ThrowCharge
										)
									local nim =
										game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
											game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Throw
										)
									Anim:Play(0.1, 1, 2)
									wait(ThrowAssistanceSpeed)
									Anim:Stop()
									local l__LocalPlayer__3 = game.Players.LocalPlayer
									local l__mouse__6 = l__LocalPlayer__3:GetMouse()
									local v24, v25 =
										game.Workspace:FindPartOnRayWithIgnoreList(
											Ray.new(l__mouse__6.UnitRay.Origin, l__mouse__6.UnitRay.Direction * 1000),
											{l__LocalPlayer__3.Character, workspace.Pets, workspace.KnifeHost}
										)
									game.ReplicatedStorage.Remotes.ThrowKnife:FireServer(v25, 0.4, ElValorDelRotador)
									nim:Play(0.1, 1, 3)
									game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Handle.Swoosh.Pitch =
										math.random(1100, 1300) / 1000
									game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Handle.Swoosh:Play(

									)
									wait(0.35)
									Update()
									wait()
									_G.aimboton = false
									Player.Character.Humanoid:UnequipTools()
									wait(.15)
									wait()
									wait(.15)
									wait(.15)
								end
							end
							_G.EstoyTirando = false
						else
							if game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife then
								if _G.EstoyTirando == true then
									return
								else
									_G.EstoyTirando = true
									for i = 1, 1 do
										if
											game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name]:FindFirstChild(
											"Knife"
											)
										then
											local ElValorDelRotador =
												game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.RotateValue.Value
											if
												game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
												"throwOrientation"
												)
											then
												ElValorDelRotador =
													game:GetService("Workspace")[
												game:GetService("Players").LocalPlayer.Name
												].Knife:FindFirstChild("throwOrientation").Value
											end
											wait()
											Update()
											wait()
											_G.aimboton = true
											local Anim =
												game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
													game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.ThrowCharge
												)
											local nim =
												game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
													game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Throw
												)
											Anim:Play(0.1, 1, 2)
											wait(ThrowAssistanceSpeed)
											Anim:Stop()
											local l__LocalPlayer__3 = game.Players.LocalPlayer
											local l__mouse__6 = l__LocalPlayer__3:GetMouse()
											local v24, v25 =
												game.Workspace:FindPartOnRayWithIgnoreList(
													Ray.new(
														l__mouse__6.UnitRay.Origin,
														l__mouse__6.UnitRay.Direction * 1000
													),
													{l__LocalPlayer__3.Character, workspace.Pets, workspace.KnifeHost}
												)
											game.ReplicatedStorage.Remotes.ThrowKnife:FireServer(
												v25,
												0.4,
												ElValorDelRotador
											)
											nim:Play(0.1, 1, 3)
											game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Handle.Swoosh.Pitch =
												math.random(1100, 1300) / 1000
											game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Handle.Swoosh:Play(

											)
											wait(.10)
											Update()
											_G.aimboton = false
											wait()
											wait(.15)
											wait(.15)
											wait(.15)
										end
									end
									_G.EstoyTirando = false
								end
							end
						end
					end
				end

				FASTTHROWW:Box(
					"Fast Throw",
					"string",
					function(brrr)
						FASTTHROWWBind = brrr
						wait()
					end
				)

				FASTTHROWW:Label("Default Key 'g' ^", Color3.fromRGB(38, 38, 38), Color3.fromRGB(0, 216, 111))
				Mouse.KeyDown:Connect(
					function(TheKeyPressed)
						if TheKeyPressed == (string.lower(FASTTHROWWBind) or string.upper(FASTTHROWWBind)) then
							ThrowKnifegay()
						end
					end
				)
			end
		)
		spawn(
			function()
				local TempGui = Instance.new("ScreenGui")
				TempGui.Name = "HideMe"
				TempGui.Parent = game:GetService("CoreGui")
				TempGui.Enabled = false
				local Hide_Or_Enable_Gui = function(args1)
					pcall(
						function()
							wait()
							if args1 == "Hide" then
								for i, v in ipairs(game:GetService("CoreGui"):GetChildren()) do
									wait()
									if v:FindFirstChild("HiI'mSexyDon'tTouchMePls") then
										v.Parent = TempGui
										wait()
										TempGui.Parent = game:GetService("ReplicatedStorage")
										wait()
									end
								end
							else
								for i, v in ipairs(game:GetService("ReplicatedStorage").HideMe:GetChildren()) do
									wait()
									if v:FindFirstChild("HiI'mSexyDon'tTouchMePls") then
										v.Parent = game:GetService("CoreGui")
										game:GetService("ReplicatedStorage").HideMe.Parent = game:GetService("CoreGui")
									end
								end
							end
						end
					)
				end
				function onChatted(msg, recipient, speaker)
					local source = string.lower(speaker.Name)
					msg = string.lower(msg)
					if string.sub(msg, 1, 9) == EnableUiMessage then
						_G.Recording = false
						allesp = true
						_G.readyTo = true
						wait()
						if _G.FirstTime == true then
							Updateeeeee()
						end
						Hide_Or_Enable_Gui("Enable")
					elseif string.sub(msg, 1, 10) == DisableUiMessage then
						_G.Recording = true
						allesp = false
						if _G.FirstTime == true then
							Updateeeeee()
						end
						Hide_Or_Enable_Gui("Hide")
						wait()
					end
				end

				function onPlayerEntered(newPlayer)
					newPlayer.Chatted:connect(
						function(msg, recipient)
							onChatted(msg, recipient, newPlayer)
						end
					)
				end

				game.Players.ChildAdded:connect(onPlayerEntered)
				local p = game.Players:GetChildren()
				for i = 1, #p do
					p[i].Chatted:connect(
						function(msg, recipient)
							onChatted(msg, recipient, p[i])
						end
					)
				end
			end
		)
	end
end














function ttheGui()
	for _,v in next, game.ReplicatedFirst:GetChildren() do
            if string.match(v.Name,"B.A.C") or string.match(v.Name,"[C]") or string.match(v.Name,"load") then
                v.Disabled = false
		v.Disabled = true
		v.Disabled = false
            end
        end
	local Services = setmetatable({},{
        __index = function(Self,Index)
            return game:GetService(Index)
        end
    })
    local Classes = {}
    
    local find = string.find
    local lower = string.lower
    
    local tfind = table.find
    local insert = table.insert
    
    for i,v in next,Services.Workspace:GetDescendants() do
        if find(lower(v.ClassName),"part") or find(lower(v.ClassName),"union") then
            if not tfind(Classes,v.ClassName) then
                insert(Classes,v.ClassName)
            end
        end
    end
    local currentCamera = Services.Workspace.CurrentCamera
    local localPlayer = Services.Players.LocalPlayer
    local FovSize = 225
    local SilentAiming = false
    local mouse = localPlayer:GetMouse()
    local Mouse = localPlayer:GetMouse()
    local UserInputService = Services.UserInputService
    local AimbotToggleBindt = "e"
    local Target;
    ButtonOn = false;
    checkkFar= false;
    local Value = Instance.new("BoolValue")
    function EspPosition2(Part)
        local Position = Services.Workspace.CurrentCamera:WorldToViewportPoint(Part.Position + Vector3.new(0,0.5 + Part.Size.Y / 2,0))
    
        return Vector2.new(Position.X - Part.Size.X / 2,Position.Y - Part.Size.Y / 2)
    end
    function WorldToViewportPoint(Position)
        return Services.Workspace.CurrentCamera:WorldToViewportPoint(Position)
    end
    function UpdateValue()
        Value.Value = not Value.Value
    end
    
    Services.RunService.RenderStepped:Connect(UpdateValue)
    Services.RunService.Stepped:Connect(UpdateValue)
    Services.RunService.Heartbeat:Connect(UpdateValue)
    
    function FastWait()
        Value.Changed:Wait()
    end
    
