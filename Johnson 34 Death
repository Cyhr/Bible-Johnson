    local MAXDISTANCE = 250;
    local ButtonOn = false;
    local aimbotkey = "Right Mouse";
    local hitp = "Head";
    local Value = Instance.new("BoolValue")
    local KnifeAim = false;
    local mouse = Services.Players.LocalPlayer:GetMouse()
    
