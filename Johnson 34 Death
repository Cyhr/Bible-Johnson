    function EspText(Part)
      	local txtname = Part.Parent.Name;
      	if txtname == "Body" then
        	txtname = Part.Parent.Parent.Name;
      	end
        local Text = Drawing.new("Text")
      	Text.Transparency = .60
        Text.Center = true
      	local turning = false;
      	spawn(function()
          	while wait(.10) do
                if #(Services.Workspace.CurrentCamera:GetPartsObscuringTarget({Part.Position},{Part.Parent})) == 0 then
                    turning = true;
                else
                    turning = false;
                end
            end
        end)
        local Connection
        Connection = Services.RunService.RenderStepped:Connect(function()
            if Part:IsDescendantOf(Services.Workspace) and not Banned_Names[txtname] and Part.Name ~= "Cosmetics" then
                if not Recording and TextEsp and (TeamCheck and NotTeam(txtname) or not TeamCheck) and (Esplockvis and Part.Parent and turning == true or not Esplockvis) then
                    Text.Position = EspPosition2(Part) - Vector2.new(0,Esptextsize / 2)
              		Text.Text = txtname
                    local Position,OnScreen = WorldToViewportPoint(Part.Position)
              		Text.Size = Esptextsize;
              		if Esptextoutline == true then Text.Outline = true; Text.OutlineColor = outlineEspcolor or Color3.fromRGB(0,0,0); else Text.Outline = false end
              		Text.Font = Esptextfont;
                    if Target and txtname == Target.Name then
                        Text.Color = TargetColor
                    elseif Sheriff and txtname == Sheriff.Name then
                        Text.Color = SheriffColor
                    else
                        Text.Color = EspColor
                    end
                    Text.Text = txtname
              		if OnScreen then
                        if checkkFar and Services.Players.LocalPlayer:DistanceFromCharacter(Part.Position) <= (EspDistance or 0) or not checkkFar then
                            Text.Visible = true
                        else
                  			Text.Visible = false
                		end
                    else
                        Text.Visible = false
                    end
                else
                    Text.Visible = false
                end
            else
                Text:Remove()
                Connection:Disconnect()
            end
        end)
    end
    
    local EspLine = function(Part)
        local Line = Drawing.new("Line")
        Line.From = Vector2.new(960,0)
        Line.Thickness = 0.1
        Line.Transparency = .60
      	local turning = false;
      	spawn(function()
          	while wait(.10) do
                if #(Services.Workspace.CurrentCamera:GetPartsObscuringTarget({Part.Position},{Part.Parent})) == 0 then
                    turning = true;
                else
                    turning = false;
                end
            end
        end)
        local Connection
        Connection = Services.RunService.RenderStepped:Connect(function()
            if Part:IsDescendantOf(Services.Workspace) and not Banned_Names[Part.Parent.Name] and not Banned_Names[Part.Parent.Parent.Name] then
                if not Recording and LineEsp and (TeamCheck and (NotTeam(Part.Parent.Name) and NotTeam(Part.Parent.Parent.Name)) or not TeamCheck) and (Esplockvis and Part.Parent and turning == true or not Esplockvis) then
                    Line.To = EspPosition(Part)
    
                    local Position,OnScreen = WorldToViewportPoint(Part.Position)
    
                    if OnScreen then
                        if checkkFar and Services.Players.LocalPlayer:DistanceFromCharacter(Part.Position) <= (EspDistance or 0) or checkkFar == false then
                            Line.Visible = true
                        else
                            Line.Visible = false
                        end
                    else
                        Line.Visible = false
                    end
    
                    if Target and Part.Parent.Name == Target.Name then
                        Line.Color = TargetColor
                    elseif Sheriff and Part.Parent.Name == Sheriff.Name then
                        Line.Color = SheriffColor
                    else
                        Line.Color = EspColor
                    end
                else
                    Line.Visible = false
                end
            else
                Line:Remove()
                Connection:Disconnect()
            end
        end)
    end
    
    local EspBox = function(Part,Part2)
        local Quad = Drawing.new("Quad")
        Quad.Thickness = 0.5
        Quad.Transparency = .60
    	local turning = false;
      	spawn(function()
          	while wait(.10) do
                if #(Services.Workspace.CurrentCamera:GetPartsObscuringTarget({Part.Position},{Part.Parent})) == 0 then
                    turning = true;
                else
                    turning = false;
                end
            end
        end)
        local Connection
        Connection = Services.RunService.RenderStepped:Connect(function()
            if Part:IsDescendantOf(Services.Workspace) and Part2:IsDescendantOf(Services.Workspace) and not Banned_Names[Part.Parent.Name] and not Banned_Names[Part.Parent.Parent.Name] then
                if not Recording and BoxEsp and (TeamCheck and (NotTeam(Part.Parent.Name) and NotTeam(Part.Parent.Parent.Name)) or not TeamCheck) and (Esplockvis and Part.Parent and turning == true or not Esplockvis) then
                    local Position,OnScreen = WorldToViewportPoint(Part2.Position)
                    if OnScreen then
                        if checkkFar and Services.Players.LocalPlayer:DistanceFromCharacter(Part.Position) <= (EspDistance or 0) or checkkFar == false then
                            local Offset = Vector3.new(2,3,0)
                            local TopLeft = WorldToViewportPoint((Part2.CFrame * CFrame.new(Offset.X,Offset.Y,0)).Position)
                            local TopRight = WorldToViewportPoint((Part2.CFrame * CFrame.new(-Offset.X,Offset.Y,0)).Position)
                            local BottomLeft = WorldToViewportPoint((Part2.CFrame * CFrame.new(Offset.X,-Offset.Y,0)).Position)
                            local BottomRight = WorldToViewportPoint((Part2.CFrame * CFrame.new(-Offset.X,-Offset.Y,0)).Position)
    						Quad.PointA = Vector2.new(TopRight.X,TopRight.Y);
                            Quad.PointB = Vector2.new(TopLeft.X,TopLeft.Y);
                            Quad.PointC = Vector2.new(BottomLeft.X,BottomLeft.Y);
                            Quad.PointD = Vector2.new(BottomRight.X,BottomRight.Y);
    						Quad.Visible = true;
                        else
                            Quad.Visible = false;
                        end
                    else
                        Quad.Visible = false;
                    end
    
                    if Target and Part.Parent.Name == Target.Name then
                        Quad.Color = TargetColor;
                    elseif Sheriff and Part.Parent.Name == Sheriff.Name then
                        Quad.Color = SheriffColor;
                    else
                        Quad.Color = EspColor;
                    end
                else
                    Quad.Visible = false;
                end
            else
                Quad:Remove()
                Connection:Disconnect()
            end
        end)
    end
    
    function UpdateEsp(Type)
        if Type == "Text" then
            spawn(function()
                for i,v in next,Services.Workspace:GetDescendants() do
                    if i % 150 == 0 then FastWait() end
                    for i2,v2 in next,Known_Names.Head do
                        if table.find(Classes,v.ClassName) and v.Parent and v.Parent.Name ~= Services.Players.LocalPlayer.Name and v.Parent.Name ~= "" and not Banned_Names[v.Parent.Name] and Known_Names.HumanoidRootPart[v.Name] and v.Parent:FindFirstChild(i2) then
                  			EspText(v.Parent[i2])
                  			break;
                        end
                    end
                end
            end)
            Services.Workspace.DescendantAdded:Connect(function(v)
            	wait()
            	if table.find(Classes,v.ClassName) and v.Parent and v and v.Parent.Name ~= Services.Players.LocalPlayer.Name and v.Parent.Name ~= "" and not Banned_Names[v.Parent.Name] and Known_Names.Head[v.Name] then
              		EspText(v)
            	end
            end)
        elseif Type == "Lines" then
            spawn(function()
                for i,v in next,Services.Workspace:GetDescendants() do
                    if i % 150 == 0 then FastWait() end
                    for k,va in pairs(Known_Names) do
                        if table.find(Classes,v.ClassName) and v.Parent and v.Parent.Name ~=Services.Players.LocalPlayer.Name and v.Parent.Name ~= "" and not Banned_Names[v.Parent.Name] and Known_Names.HumanoidRootPart[v.Name] and k == "Head" and v.Parent:FindFirstChild(va[k]) then
                            local one = v.Parent.Head
                            EspLine(one)
                        end
                    end
                end
            end)
            Services.Workspace.DescendantAdded:Connect(function(v)
                wait()
                for k,va in pairs(Known_Names) do
                    if table.find(Classes,v.ClassName) and v.Parent and v.Parent.Name ~=Services.Players.LocalPlayer.Name and v.Parent.Name ~= "" and not Banned_Names[v.Parent.Name] and Known_Names.HumanoidRootPart[v.Name] and k == "Head" and v.Parent:FindFirstChild(va[k]) then
                        local one = v.Parent.Head
                        EspLine(one)
                    end
                end
            end)
        elseif Type == "Boxes" then
            spawn(function()
                for i,v in next,Services.Workspace:GetDescendants() do
                    if i % 150 == 0 then FastWait() end
                    for k,va in pairs(Known_Names) do
                        if table.find(Classes,v.ClassName) and v.Parent and v.Parent.Name ~=Services.Players.LocalPlayer.Name and v.Parent.Name ~= "" and not Banned_Names[v.Parent.Name] and Known_Names.HumanoidRootPart[v.Name] and k == "Head" and v.Parent:FindFirstChild(va[k]) then
                            local one = v.Parent.Head --Head
                            local two = v --RootPart
                            EspBox(one,two)
                        end
                    end
                end
            end)
            Services.Workspace.DescendantAdded:Connect(function(v)
                wait()
                for k,va in pairs(Known_Names) do
                    if table.find(Classes,v.ClassName) and v.Parent and v.Parent.Name ~=Services.Players.LocalPlayer.Name and v.Parent.Name ~= "" and not Banned_Names[v.Parent.Name] and Known_Names.HumanoidRootPart[v.Name] and k == "Head" and v.Parent:FindFirstChild(va[k]) then
                        local one = v.Parent.Head
                        local two = v
                        EspBox(one,two)
                    end
                end
            end)
        end
    end
    
    espsettings:Button("ALL",function()
        UpdateEsp("Text")
        UpdateEsp("Lines")
        UpdateEsp("Boxes")
    end)
    
    espsettings:Button("Tracers",function()
         UpdateEsp("Lines")
    end)
    
    espsettings:Button("Boxes",function()
        UpdateEsp("Boxes")
    end)
    
    espsettings:Button("Names",function()
        UpdateEsp("Text")
    end)
    
    local Old = Get_Closest_Head()
    getgenv().Updatee = function()
        if hitp == "Head" then
            Old = Get_Closest_Head()
        else
        	Old = Get_Closest_Root()
        end
    end
    
    getgenv().Update = function()
        if Get_Closest_Head() == Services.Players.LocalPlayer then
        	Updatee()
        else
        	Updatee()
      	end
        if Get_Closest_Root() == Services.Players.LocalPlayer then
        	Updatee()
        else
        	Updatee()
      	end
    end
    
    UserInputService.InputBegan:Connect(function(Input)
        if Input.UserInputType == Enum.UserInputType.MouseButton2 then
        if aimbotkey == "Right Mouse" then
            getgenv().aimboton = true
            Update()
        end
        end
    end)
    
    UserInputService.InputEnded:Connect(function(Input)
        if Input.UserInputType == Enum.UserInputType.MouseButton2 then
        if aimbotkey == "Right Mouse" then
            getgenv().aimboton = false
        end
        end
    end)
    
    UserInputService.InputBegan:Connect(function(Input)
        if Input.UserInputType == Enum.UserInputType.MouseButton1 then
            if aimbotkey == "Left Mouse" then
                getgenv().aimboton = true
                Update()
            end
        end
    end)
    
    UserInputService.InputEnded:Connect(function(Input)
        if Input.UserInputType == Enum.UserInputType.MouseButton1 then
            if aimbotkey == "Left Mouse" then
                getgenv().aimboton = false
            end
        end
    end)
    
    spawn(function()
        local turning = false;
      	spawn(function()
          	while wait(.06) do
                if Get_Closest_Root() ~= Services.Players.LocalPlayer and Get_Closest_Head() ~= Services.Players.LocalPlayer and Old and Old.Parent and Old ~= Services.Players.LocalPlayer and #(Services.Workspace.CurrentCamera:GetPartsObscuringTarget({Old.Position},{Old.Parent})) == 0 then
                    turning = true;
                else
                    turning = false;
                end
            end
        end)
        game:GetService("RunService").Stepped:Connect(function()
            spawn(function()
                if EspDistance == nil then
                    EspDistance = math.random(1200)
                end
            end)
            Update()
            if ButtonOn == true and Get_Closest_Root() ~= Services.Players.LocalPlayer and Get_Closest_Head() ~= Services.Players.LocalPlayer and Old and Old.Parent and Old ~= Services.Players.LocalPlayer then
                if getgenv().aimboton == true and (checkkFar and Services.Players.LocalPlayer:DistanceFromCharacter(Old.Position) <= EspDistance or checkkFar == false) then
                    local g, onscreen = Services.Workspace.CurrentCamera:WorldToScreenPoint(Old.Position)
                    if KnifeAim == true then
                        local aimsss;
                        local Head =  Old
                        local Velocityx = Head.Velocity.X
                        local Velocityy = Head.Velocity.Y
                        local Velocityz = Head.Velocity.Z
                        if Services.Players.LocalPlayer:DistanceFromCharacter(Old.Position) <= 20 then
                        aimsss =
                            Head.Position +
                            Vector3.new(
                            (Velocityx >= 1 and 1) or (Velocityx <= -1 and -1) or 0,
                            (Velocityy >= 1 and 1) or (Velocityy <= -1 and -1) or 0,
                            (Velocityz >= 1 and 1) or (Velocityz <= -1 and -1) or 0
                            )
                        elseif Services.Players.LocalPlayer:DistanceFromCharacter(Old.Position) >= 20 then
                        aimsss =
                            Head.Position +
                            Vector3.new(
                            (Velocityx >= 2.5 and 2.5) or (Velocityx <= -2.5 and -2.5) or 0,
                            (Velocityy >= 2.5 and 2.5) or (Velocityy <= -2.5 and -2.5) or 0,
                            (Velocityz >= 2.5 and 2.5) or (Velocityz <= -2.5 and -2.5) or 0
                            )
                        elseif Services.Players.LocalPlayer:DistanceFromCharacter(Old.Position) <= 40 then
                        aimsss =
                            Head.Position +
                            Vector3.new(
                            (Velocityx >= 3 and 3) or (Velocityx <= -3 and -3) or 0,
                            (Velocityy >= 3 and 3) or (Velocityy <= -3 and -3) or 0,
                            (Velocityz >= 3 and 3) or (Velocityz <= -3 and -3) or 0
                            )
                        elseif Services.Players.LocalPlayer:DistanceFromCharacter(Old.Position) >= 40 then
                        aimsss =
                            Head.Position +
                            Vector3.new(
                            (Velocityx >= 3.5 and 3.5) or (Velocityx <= -3.5 and -3.5) or 0,
                            (Velocityy >= 3.5 and 3.5) or (Velocityy <= -3.5 and -3.5) or 0,
                            (Velocityz >= 3.5 and 3.5) or (Velocityz <= -3.5 and -3.5) or 0
                            )
                        elseif Services.Players.LocalPlayer:DistanceFromCharacter(Old.Position) <= 60 then
                        aimsss =
                            Head.Position +
                            Vector3.new(
                            (Velocityx >= 4 and 4) or (Velocityx <= -4 and -4) or 4,
                            (Velocityy >= 4 and 4) or (Velocityy <= -4 and -4) or 4,
                            (Velocityz >= 4 and 4) or (Velocityz <= -4 and -4) or 4
                            )
                        elseif Services.Players.LocalPlayer:DistanceFromCharacter( Old.Position) >= 60 then
                        aimsss =
                            Head.Position +
                            Vector3.new(
                            (Velocityx >= 4.5 and 4.5) or (Velocityx <= -4.5 and -4.5) or 0,
                            (Velocityy >= 4.5 and 4.5) or (Velocityy <= -4.5 and -4.5) or 0,
                            (Velocityz >= 4.5 and 4.5) or (Velocityz <= -4.5 and -4.5) or 0
                            )
                        elseif Services.Players.LocalPlayer:DistanceFromCharacter(Old.Position) <= 80 then
                        aimsss =
                            Head.Position +
                            Vector3.new(
                            (Velocityx >= 5 and 5) or (Velocityx <= -5 and -5) or 0,
                            (Velocityy >= 5 and 5) or (Velocityy <= -5 and -5) or 0,
                            (Velocityz >= 5 and 5) or (Velocityz <= -5 and -5) or 0
                            )
                        elseif Services.Players.LocalPlayer:DistanceFromCharacter(Old.Position) >= 80 then
                            aimsss =
                            Head.Position +
                            Vector3.new(
                            (Velocityx >= 5.5 and 5.5) or (Velocityx <= -5.5 and -5.5) or 0,
                            (Velocityy >= 5.5 and 5.5) or (Velocityy <= -5.5 and -5.5) or 0,
                            (Velocityz >= 5.5 and 5.5) or (Velocityz <= -5.5 and -5.5) or 0
                            )
                        end
                        pcall(function() if onscreen and Old and (TeamCheck and (NotTeam(Old.Parent.Name) and NotTeam(Old.Parent.Parent.Name)) or not TeamCheck) and (Esplockvis and Old and turning == true or not Esplockvis) then
                            return mousemoverel(Services.Workspace.CurrentCamera:WorldToScreenPoint(aimsss).X - Services.Workspace.CurrentCamera:WorldToScreenPoint(mouse.Hit.p).X + aimbotxpos,Services.Workspace.CurrentCamera:WorldToScreenPoint(aimsss).Y - Services.Workspace.CurrentCamera:WorldToScreenPoint(mouse.Hit.p).Y + aimbotypos)
                        end end)
                    else
                        aimsss = Old.Position;
                        pcall(function() if onscreen and Old and (TeamCheck and (NotTeam(Old.Parent.Name) and NotTeam(Old.Parent.Parent.Name)) or not TeamCheck) and (Esplockvis and Old and turning == true or not Esplockvis) then
                            return mousemoverel(Services.Workspace.CurrentCamera:WorldToScreenPoint(aimsss).X - Services.Workspace.CurrentCamera:WorldToScreenPoint(mouse.Hit.p).X + aimbotxpos,Services.Workspace.CurrentCamera:WorldToScreenPoint(aimsss).Y - Services.Workspace.CurrentCamera:WorldToScreenPoint(mouse.Hit.p).Y + aimbotypos)
                        end end)
                    end
                end
            end
        end)
    end)
    
    spawn(function()
        repeat
            wait()
        until allesp
        UpdateEsp("Text")
        UpdateEsp("Lines")
        UpdateEsp("Boxes")
        local LineEsp = false;
        local BoxEsp = false;
        local TextEsp = false;
    end)
    
    function LoadAssassinSettings()
        TextEsp = Show_Text_Esp_Toggle or false;
        BoxEsp = Show_Boxes_Esp_Toggle or false;
        LineEsp = Show_Lines_Esp_Toggle or false;
        allesp = Show_All_Esp_Toggle or false;
        aimbotkey = Aim_Key or "Right Mouse"
        KnifeAim = Knife_Style_Locking_Toggle or false;
        showfovaimbot = Show_Show_Fov_When_Active_Toggle or false;
        Esplockvis = Visibility_Check  or false;
        hitp = Aim_At or "Head";
        MAXDISTANCE = Circle_Fov_Size or 250;
    end
    
    if game.CreatorId == 113657341 then
        LoadAssassinSettings()
    end
end

