		Mouse.KeyDown:Connect(
			function(TheKeyPressed)
				if TheKeyPressed == (string.lower(AimbotToggleBindt) or string.upper(AimbotToggleBindt)) then
					if ButtonOn == false then
						ButtonOn = true
					else
						ButtonOn = false
					end
				end
			end
		)

		local grab_plr = function()
			if eltargetthinfxd == "FirstPerson" then
				return FirstPerson()
			elseif eltargetthinfxd == "ThirdPerson" then
				return ThirdPerson()
			elseif eltargetthinfxd == "ClosestPlrMouse" then
				return getClosestPlayerToCursor()
			end
		end

		local RandomPlayer = function()
			return game:GetService("Players"):GetPlayers()[math.random(1, #game:GetService("Players"):GetPlayers())]
		end

		local Old = RandomPlayer().Name
		local Update = function()
			Old = grab_plr().Name
		end

		local GetYes = function()
			local gonchang
			pcall(
				function()
					if game:GetService("Players"):FindFirstChild(Old) then
						gonchang = game:GetService("Players"):FindFirstChild(Old)
					else
						gonchang = game:GetService("Players"):FindFirstChild(RandomPlayer().Name)
					end
				end
			)
			return gonchang
		end

		local hasProperty = function(object, prop)
			local t = object[prop]
		end

		local Render_Yes = function()
			settings().Rendering.QualityLevel = "Level01"
			for i, v in pairs(game:GetDescendants()) do
				local success =
					pcall(
						function()
							hasProperty(v, "Material")
						end
					)
				if success and v.Name ~= "HumanoidRootPart" then
					v.Material = "Plastic"
				end
			end
			game:GetService("Workspace").DescendantAdded:Connect(
			function(v)
				wait()
				local success =
					pcall(
						function()
							hasProperty(v, "Material")
						end
					)
				if success and v.Name ~= "HumanoidRootPart" then
					v.Material = "Plastic"
				end
			end
			)
		end

		local FpsBoost = function()
			Render_Yes()
		end

		JONCOMP:Button(
			"Join Competitive",
			function()
				local JoinCompetitive = function(args1, args2, args3)
					game:GetService("ReplicatedStorage").Remotes.MatchmakingRemotes.PlayerJoinLobby:FireServer(
					args1,
					args2,
					args3
					)
				end
				JoinCompetitive("#Competitive", false, "solo")
			end
		)

		SPEEDi:Slider(
			"SA Speed",
			.10,
			.70,
			true,
			function(value)
				Silent_AimSpeed = value
			end
		)
		SPEEDi:Slider(
			"Spam SA Speed",
			.10,
			.70,
			true,
			function(value)
				SpammableSilentAimSpeed = value
			end
		)
		SPEEDi:Slider(
			"Throw Asst Speed",
			.10,
			.70,
			true,
			function(value)
				ThrowAssistanceSpeed = value
			end
		)

		local walking_slow
		local myspeed = 16

		spawn(
			function()
				while wait() do
					pcall(
						function()
							if walking_slow == true then
								game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = myspeed
							else
								game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = 16
							end
						end
					)
				end
			end
		)

		walkslow:Toggle(
			"Slow Walk",
			function(bb)
				walking_slow = bb
			end
		)

		walkslow:Slider(
			"Speed",
			0,
			10,
			true,
			function(value)
				myspeed = value
			end
		)

		fpsboost:Button(
			"Boost! (face kill)",
			function()
				FpsBoost()
			end
		)

		fpsboost:Button(
			"Boost! (no face kill)",
			function()
				local Render_Yes = function()
					for i, v in pairs(game:GetDescendants()) do
						if v.ClassName == ("Decal") and v.Name ~= "face" then
							v:Destroy()
						end
						if v.ClassName == ("Texture") then
							v:Destroy()
						end
						if v.ClassName == ("ParticleEmitter") or v.ClassName == ("Trail") then
							v.Lifetime = NumberRange.new(0)
						end
						if v.ClassName == ("Explosion") then
							v.BlastPressure = 1
							v.BlastRadius = 1
						end
						if
							v.ClassName == ("Fire") or v.ClassName == ("SpotLight") or v.ClassName == ("Smoke") or
							v.ClassName == ("Sparkles")
						then
							v.Enabled = false
						end
						if
							v.ClassName == ("BlurEffect") or v.ClassName == ("SunRaysEffect") or
							v.ClassName == ("ColorCorrectionEffect") or
							v.ClassName == ("BloomEffect") or
							v.ClassName == ("DepthOfFieldEffect")
						then
							v.Enabled = false
						end
					end
					game.DescendantAdded:Connect(
						function(v)
							wait()
							if v.ClassName == ("Decal") and v.Name ~= "face" then
								v:Destroy()
							end
							if v.ClassName == ("Texture") then
								v:Destroy()
							end
							if v.ClassName == ("ParticleEmitter") or v.ClassName == ("Trail") then
								v.Lifetime = NumberRange.new(0)
							end
							if v.ClassName == ("Explosion") then
								v.BlastPressure = 1
								v.BlastRadius = 1
							end
							if
								v.ClassName == ("Fire") or v.ClassName == ("SpotLight") or v.ClassName == ("Smoke") or
								v.ClassName == ("Sparkles")
							then
								v.Enabled = false
							end
							if
								v.ClassName == ("BlurEffect") or v.ClassName == ("SunRaysEffect") or
								v.ClassName == ("ColorCorrectionEffect") or
								v.ClassName == ("BloomEffect") or
								v.ClassName == ("DepthOfFieldEffect")
							then
								v.Enabled = false
							end
						end
					)
				end
				settings().Rendering.QualityLevel = "Level01"
				sethiddenproperty(game.Lighting, "Technology", 2)
				sethiddenproperty(game.Workspace.Terrain, "Decoration", false)
				game.Lighting.GlobalShadows = false
				game.Lighting.FogEnd = 9e9
				game.Lighting.Brightness = 0
				Render_Yes()
			end
		)

		local ClanUi = false
		showclan:Toggle(
			"Show Clan Pts",
			function(bb)
				ClanUi = bb
			end
		)

		chatt:Box(
			"Enable Gui",
			"string",
			function(brrr)
				EnableUiMessage = brrr
			end
		)
		chatt:Label("Default mmsg '/e enable'", Color3.fromRGB(38, 38, 38), Color3.fromRGB(0, 216, 111))
		chatt:Box(
			"Hide Gui",
			"string",
			function(brrr)
				DisableUiMessage = brrr
			end
		)
		chatt:Label("Default mmsg '/e disable'", Color3.fromRGB(38, 38, 38), Color3.fromRGB(0, 216, 111))
		spawn(
			function()
				local Grab_Clan_Info = function(Player_Name, Requesting_Info)
					for i, v in next, game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.UI.ClanHub.InfoPage.PlayerHold.GlobalGrid:GetDescendants(

						) do
						if v:IsA("Frame") and v.Name == "ClanViewSlot" then
							if v:FindFirstChild("TextLabel").Text == Player_Name then
								if Requesting_Info == 1 then
									return v.alltimepts.TextLabel.Text
								else
									if v.alltimepts.Visible == true then
										return true
									else
										return false
									end
								end
							end
						end
					end
				end

				local Ready = false
				spawn(
					function()
						while wait() do
							pcall(
								function()
									if Grab_Clan_Info(game:GetService("Players").LocalPlayer.Name, 2) == true then
										Ready = true
									end
								end
							)
							if Ready == true then
								break
							end
						end
					end
				)

				repeat
					wait()
				until Ready == true

				local PA = Grab_Clan_Info(game:GetService("Players").LocalPlayer.Name, 1)
				local PuntosActiguos = PA

				local ScreenGui111 = Instance.new("ScreenGui")
				local Frame111 = Instance.new("Frame")
				local oldpointslabel = Instance.new("TextLabel")
				local OldPoints = Instance.new("TextLabel")
				local newpointslabel = Instance.new("TextLabel")
				local NewPoints = Instance.new("TextLabel")
				local TextLabel = Instance.new("TextLabel")
				ScreenGui111.Parent = game.CoreGui
				Frame111.Parent = ScreenGui111
				Frame111.BackgroundColor3 = Color3.fromRGB(75, 75, 75)
				Frame111.BackgroundTransparency = 0.500
				Frame111.Position = UDim2.new(0, 0, 0.611042917, 0)
				Frame111.Size = UDim2.new(0, 342, 0, 121)
				oldpointslabel.Name = "oldpointslabel"
				oldpointslabel.Parent = Frame111
				oldpointslabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				oldpointslabel.BackgroundTransparency = 1.000
				oldpointslabel.Size = UDim2.new(0, 179, 0, 51)
				oldpointslabel.Font = Enum.Font.SourceSans
				oldpointslabel.Text = "Points:"
				oldpointslabel.TextColor3 = Color3.fromRGB(0, 0, 0)
				oldpointslabel.TextScaled = true
				oldpointslabel.TextSize = 14.000
				oldpointslabel.TextWrapped = true
				OldPoints.Name = "OldPoints"
				OldPoints.Parent = Frame111
				OldPoints.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				OldPoints.BackgroundTransparency = 1.000
				OldPoints.Position = UDim2.new(0.332702994, 0, 0, 0)
				OldPoints.Size = UDim2.new(0, 228, 0, 51)
				OldPoints.Font = Enum.Font.SourceSans
				OldPoints.Text = "+ " .. Grab_Clan_Info(game:GetService("Players").LocalPlayer.Name, 1)
				OldPoints.TextColor3 = Color3.fromRGB(153, 0, 0)
				OldPoints.TextScaled = true
				OldPoints.TextSize = 14.000
				OldPoints.TextWrapped = true
				newpointslabel.Name = "newpointslabel"
				newpointslabel.Parent = Frame111
				newpointslabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				newpointslabel.BackgroundTransparency = 1.000
				newpointslabel.Position = UDim2.new(-0.00150051364, 0, 0.574581206, 0)
				newpointslabel.Size = UDim2.new(0, 203, 0, 68)
				newpointslabel.Font = Enum.Font.SourceSans
				newpointslabel.Text = "Points Gathered:"
				newpointslabel.TextColor3 = Color3.fromRGB(0, 0, 0)
				newpointslabel.TextScaled = true
				newpointslabel.TextSize = 14.000
				newpointslabel.TextWrapped = true
				NewPoints.Name = "NewPoints"
				NewPoints.Parent = Frame111
				NewPoints.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				NewPoints.BackgroundTransparency = 1.000
				NewPoints.Position = UDim2.new(0.523391783, 0, 0.640697122, 0)
				NewPoints.Size = UDim2.new(0, 162, 0, 50)
				NewPoints.Font = Enum.Font.SourceSans
				NewPoints.Text =
					"+ " .. (Grab_Clan_Info(game:GetService("Players").LocalPlayer.Name, 1) - PuntosActiguos)
				NewPoints.TextColor3 = Color3.fromRGB(153, 0, 0)
				NewPoints.TextScaled = true
				NewPoints.TextSize = 14.000
				NewPoints.TextWrapped = true
				local Hours = 0
				local Minutes = 0
				local Seconds = 0
				TextLabel.Parent = Frame111
				TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				TextLabel.BackgroundTransparency = 1.000
				TextLabel.Position = UDim2.new(-0.00259738509, 0, 0.417154104, 0)
				TextLabel.Size = UDim2.new(0, 342, 0, 41)
				TextLabel.Font = Enum.Font.SourceSans
				TextLabel.Text = ("Time Played!:" .. Hours .. "H," .. Minutes .. "M," .. Seconds .. "S.")
				TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
				TextLabel.TextScaled = true
				TextLabel.TextSize = 14.000
				TextLabel.TextWrapped = true
				spawn(
					function()
						while wait(1) do
							Seconds = Seconds + 1
							if Seconds == 60 then
								Seconds = 0
								Minutes = Minutes + 1
							end
							if Minutes == 60 then
								Minutes = 0
								Hours = Hours + 1
							end
							TextLabel.Text =
								("Time Played!: " .. Hours .. " H, " .. Minutes .. " M, " .. Seconds .. " S.")
						end
					end
				)

				while wait() do
					if ClanUi == true then
						Frame111.Visible = true
						pcall(
							function()
								OldPoints.Text = "+ " .. Grab_Clan_Info(game:GetService("Players").LocalPlayer.Name, 1)
								NewPoints.Text =
									"+ " ..
									(Grab_Clan_Info(game:GetService("Players").LocalPlayer.Name, 1) - PuntosActiguos)
							end
						)
					else
						Frame111.Visible = false
					end
				end
			end
		)

