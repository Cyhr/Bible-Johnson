            "ESP Color",
            Color3.fromRGB(53, 220, 53),
            function(color)
                allespcolor = color
            end
        )

        local ESPText = function(part)
            spawn(
                function()
                    local name = Drawing.new("Text")
                    name.Text = part.Parent.Name
                    name.Position = ESPPOS(part)
                    name.Size = 20.00
                    name.Outline = true
                    name.Center = true
                    name.Visible = true
                    local readytoleave = false
                    local ReadyToDisc;
                    local ActualHook;
                    ActualHook = game:GetService("RunService").Heartbeat:Connect(function()
                        if name ~= nil then
                            if part:IsDescendantOf(game:GetService("Workspace")) then
                                if allesp == true then
                                    if _G.Recording == false then
                                        if names == true then
                                            name.Position = ESPPOS(part)
                                            local _, screen = Work.CurrentCamera:WorldToViewportPoint(part.Position)
                                            if screen then
                                                if localPlayer:DistanceFromCharacter(part.Position) <= 1200 then
                                                    name.Visible = true
                                                else
                                                    name.Visible = false
                                                end
                                            else
                                                name.Visible = false
                                            end
                                            if allespcolor then
                                                if MyTarget() and part.Parent.Name == MyTarget().Name then
                                                    name.Color = targetespcolor
                                                else
                                                    name.Color = allespcolor
                                                end
                                            end
                                        else
                                            name.Visible = false
                                        end
                                    else
                                        name.Visible = false
                                    end
                                else
                                    name.Visible = false
                                end
                            else
                                name.Visible = false
                                ReadyToDisc = true
                            end
                        else
                            name.Visible = false
                            ReadyToDisc = true
                        end
                        if ReadyToDisc == true then
                            name:Remove()
                            readytoleave = true
                        end
                        if readytoleave == true then
                            ActualHook:Disconnect()
                            
                        end
                    end)
                end
            )
        end

        local ESPLine = function(part)
            spawn(
                function()
                    local Line = Drawing.new("Line")
                    Line.Visible = true
                    Line.From = Vector2.new(1000, 15)
                    Line.To = ESPPOS(part)
                    Line.Thickness = 2
                    Line.Transparency = 1
                    local readytoleave = false
                    local ReadyToDisc;
                    local ActualHook;
                    ActualHook = game:GetService("RunService").Stepped:Connect(function()
                        if Line ~= nil then
                            if part:IsDescendantOf(game:GetService("Workspace")) then
                                if allesp == true then
                                    if _G.Recording == false then
                                        if tracers == true then
                                            Line.To = ESPPOS(part)
                                            local _, screen = Work.CurrentCamera:WorldToViewportPoint(part.Position)
                                            if screen then
                                                if localPlayer:DistanceFromCharacter(part.Position) <= 1200 then
                                                    Line.Visible = true
                                                else
                                                    Line.Visible = false
                                                end
                                            else
                                                Line.Visible = false
                                            end
                                            if allespcolor then
                                                if MyTarget() and part.Parent.Name == MyTarget().Name then
                                                    Line.Color = targetespcolor
                                                else
                                                    Line.Color = allespcolor
                                                end
                                            end
                                        else
                                            Line.Visible = false
                                        end
                                    else
                                        Line.Visible = false
                                    end
                                else
                                    Line.Visible = false
                                end
                            else
                                Line.Visible = false
                                ReadyToDisc = true
                            end
                        else
                            Line.Visible = false
                            ReadyToDisc = true
                        end
                        if ReadyToDisc == true then
                            Line:Remove()
                            readytoleave = true
                        end
                        if readytoleave == true then
                            ActualHook:Disconnect()
                            
                        end
                    end)
                end
            )
        end

        local plr = game.Players.LocalPlayer
        local camera = game.Workspace.CurrentCamera
        local ESPSquare = function(part, part2)
            spawn(
                function()
                    local Top = Drawing.new("Line")
                    Top.Visible = false
                    Top.From = Vector2.new(0, 0)
                    Top.To = Vector2.new(200, 200)
                    Top.Thickness = 2
                    Top.Transparency = 1

                    local Bottom = Drawing.new("Line")
                    Bottom.Visible = false
                    Bottom.From = Vector2.new(0, 0)
                    Bottom.To = Vector2.new(200, 200)
                    Bottom.Thickness = 2
                    Bottom.Transparency = 1

                    local Left = Drawing.new("Line")
                    Left.Visible = false
                    Left.From = Vector2.new(0, 0)
                    Left.To = Vector2.new(200, 200)
                    Left.Color = Color3.fromRGB(255, 0, 0)
                    Left.Thickness = 2
                    Left.Transparency = 1

                    local Right = Drawing.new("Line")
                    Right.Visible = false
                    Right.From = Vector2.new(0, 0)
                    Right.To = Vector2.new(200, 200)
                    Right.Thickness = 2
                    Right.Transparency = 1
                    local readytoleave = false
                    local ReadyToDisc;
                    local ActualHook;
                    ActualHook = game:GetService("RunService").Heartbeat:Connect(function()
                        if Top ~= nil and Left ~= nil and Bottom ~= nil and Right ~= nil then
                            if part2:IsDescendantOf(game:GetService("Workspace")) then
                                if allesp == true then
                                    if _G.Recording == false then
                                        if CreateTextboxes == true then
                                            local ScreenPos, OnScreen = camera:WorldToViewportPoint(part2.Position)
                                            if OnScreen then
                                                local Scale = part.Size.Y / 2
                                                local Size = Vector3.new(2, 3, 0) * (Scale * 2)
                                                local humpos = camera:WorldToViewportPoint(part2.Position)
                                                local TL =
                                                    camera:WorldToViewportPoint(
                                                    (part2.CFrame * CFrame.new(Size.X, Size.Y, 0)).p
                                                )
                                                local TR =
                                                    camera:WorldToViewportPoint(
                                                    (part2.CFrame * CFrame.new(-Size.X, Size.Y, 0)).p
                                                )
                                                local BL =
                                                    camera:WorldToViewportPoint(
                                                    (part2.CFrame * CFrame.new(Size.X, -Size.Y, 0)).p
                                                )
                                                local BR =
                                                    camera:WorldToViewportPoint(
                                                    (part2.CFrame * CFrame.new(-Size.X, -Size.Y, 0)).p
                                                )
                                                Top.From = Vector2.new(TL.X, TL.Y)
                                                Top.To = Vector2.new(TR.X, TR.Y)
                                                Left.From = Vector2.new(TL.X, TL.Y)
                                                Left.To = Vector2.new(BL.X, BL.Y)
                                                Right.From = Vector2.new(TR.X, TR.Y)
                                                Right.To = Vector2.new(BR.X, BR.Y)
                                                Bottom.From = Vector2.new(BL.X, BL.Y)
                                                Bottom.To = Vector2.new(BR.X, BR.Y)
                                                if localPlayer:DistanceFromCharacter(part2.Position) <= 1200 then
                                                    Top.Visible = true
                                                    Left.Visible = true
                                                    Bottom.Visible = true
                                                    Right.Visible = true
                                                else
                                                    Top.Visible = false
                                                    Left.Visible = false
                                                    Bottom.Visible = false
                                                    Right.Visible = false
                                                end
                                                if allespcolor then
                                                    if MyTarget() and part.Parent.Name == MyTarget().Name then
                                                        Top.Color = targetespcolor
                                                        Left.Color = targetespcolor
                                                        Bottom.Color = targetespcolor
                                                        Right.Color = targetespcolor
                                                    else
                                                        Top.Color = allespcolor
                                                        Left.Color = allespcolor
                                                        Bottom.Color = allespcolor
                                                        Right.Color = allespcolor
                                                    end
                                                end
                                            else
                                                Top.Visible = false
                                                Left.Visible = false
                                                Bottom.Visible = false
                                                Right.Visible = false
                                            end
                                        else
                                            Top.Visible = false
                                            Left.Visible = false
                                            Bottom.Visible = false
                                            Right.Visible = false
                                        end
                                    else
                                        Top.Visible = false
                                        Left.Visible = false
                                        Bottom.Visible = false
                                        Right.Visible = false
                                    end
                                else
                                    Top.Visible = false
                                    Left.Visible = false
                                    Bottom.Visible = false
                                    Right.Visible = false
                                end
                            else
                                Top.Visible = false
                                Left.Visible = false
                                Bottom.Visible = false
                                Right.Visible = false
                                ReadyToDisc = true
                            end
                        else
                            Top.Visible = false
                            Left.Visible = false
                            Bottom.Visible = false
                            Right.Visible = false
                            ReadyToDisc = true
                        end
                        if ReadyToDisc == true then
                            Top:Remove()
                            Left:Remove()
                            Bottom:Remove()
                            Right:Remove()
                            readytoleave = true
                        end
                        if readytoleave == true then
                            ActualHook:Disconnect()
                            
                        end
                    end)
                end
            )
        end

        local ToEsp = false
        local UpdtEsp = function(Thing)
            if Thing == "All" then
                for i, v in pairs(game:GetDescendants()) do
                    if
                        v:IsA("Part") and v.Name == "HumanoidRootPart" and
                            v.Parent.Name ~= game.Players.LocalPlayer.Name and
                            v.Name ~= "" and
                            v.Parent.Name ~= ""
                     then
                        ESPText(v.Parent.Head)
                        ESPLine(v.Parent.Head)
                        ESPSquare(v.Parent.Head, v)
                    end
                end
                if ToEsp == false then
                    ToEsp = true
                    game.DescendantAdded:Connect(
                        function(v)
                            wait()
                            if
                                v:IsA("Part") and v.Name == "HumanoidRootPart" and
                                    v.Parent.Name ~= game.Players.LocalPlayer.Name and
                                    v.Name ~= "" and
                                    v.Parent.Name ~= ""
                             then
                                ESPText(v.Parent.Head)
                                ESPLine(v.Parent.Head)
                                ESPSquare(v.Parent.Head, v)
                            end
                        end
                    )
                end
            elseif Thing == "Names" then
                for i, v in pairs(game:GetDescendants()) do
                    if
                        v:IsA("Part") and v.Name == "HumanoidRootPart" and
                            v.Parent.Name ~= game.Players.LocalPlayer.Name and
                            v.Name ~= "" and
                            v.Parent.Name ~= ""
                     then
                        ESPText(v.Parent.Head)
                    end
                end
                if ToEsp == false then
                    ToEsp = true
                    game.DescendantAdded:Connect(
                        function(v)
                            wait()
                            if
                                v:IsA("Part") and v.Name == "HumanoidRootPart" and
                                    v.Parent.Name ~= game.Players.LocalPlayer.Name and
                                    v.Name ~= "" and
                                    v.Parent.Name ~= ""
                             then
                                ESPText(v.Parent.Head)
                            end
                        end
                    )
                end
            elseif Thing == "Tracers" then
                for i, v in pairs(game:GetDescendants()) do
                    if
                        v:IsA("Part") and v.Name == "HumanoidRootPart" and
                            v.Parent.Name ~= game.Players.LocalPlayer.Name and
                            v.Name ~= "" and
                            v.Parent.Name ~= ""
                     then
                        ESPLine(v.Parent.Head)
                    end
                end
                if ToEsp == false then
                    ToEsp = true
                    game.DescendantAdded:Connect(
                        function(v)
                            wait()
                            if
                                v:IsA("Part") and v.Name == "HumanoidRootPart" and
                                    v.Parent.Name ~= game.Players.LocalPlayer.Name and
                                    v.Name ~= "" and
                                    v.Parent.Name ~= ""
                             then
                                ESPLine(v.Parent.Head)
                            end
                        end
                    )
                end
            elseif Thing == "Boxes" then
                for i, v in pairs(game:GetDescendants()) do
                    if
                        v:IsA("Part") and v.Name == "HumanoidRootPart" and
                            v.Parent.Name ~= game.Players.LocalPlayer.Name and
                            v.Name ~= "" and
                            v.Parent.Name ~= ""
                     then
                        ESPSquare(v.Parent.Head, v)
                    end
                end
                if ToEsp == false then
                    ToEsp = true
                    game.DescendantAdded:Connect(
                        function(v)
                            wait()
                            if
                                v:IsA("Part") and v.Name == "HumanoidRootPart" and
                                    v.Parent.Name ~= game.Players.LocalPlayer.Name and
                                    v.Name ~= "" and
                                    v.Parent.Name ~= ""
                             then
                                ESPSquare(v.Parent.Head, v)
                            end
                        end
                    )
                end
            end
        end

        espsettings:Button(
            "ALL",
            function()
                UpdtEsp("All")
            end
        )

        espsettings:Button(
            "Tracers",
            function()
                UpdtEsp("Tracers")
            end
        )

        espsettings:Button(
            "Boxes",
            function()
                UpdtEsp("Boxes")
            end
        )

        espsettings:Button(
            "Names",
            function()
                UpdtEsp("Names")
            end
        )

        UserInputService.InputBegan:Connect(
            function(Input)
                if Input.UserInputType == Enum.UserInputType.MouseButton2 then
                    if aimbotkey == "Right Mouse" then
                        Update()
                        _G.aimboton = true
                    end
                end
            end
        )

        UserInputService.InputEnded:Connect(
            function(Input)
                if Input.UserInputType == Enum.UserInputType.MouseButton2 then
                    if aimbotkey == "Right Mouse" then
                        _G.aimboton = false
                    end
                end
            end
        )

        UserInputService.InputBegan:Connect(
            function(Input)
                if Input.UserInputType == Enum.UserInputType.MouseButton1 then
                    if aimbotkey == "Left Mouse" then
                        Update()
                        _G.aimboton = true
                    end
                end
            end
        )

        UserInputService.InputEnded:Connect(
            function(Input)
                if Input.UserInputType == Enum.UserInputType.MouseButton1 then
                    if aimbotkey == "Left Mouse" then
                        _G.aimboton = false
                    end
                end
            end
        )

        spawn(
            function()
                game:GetService("RunService").Stepped:Connect(
                    function()
                        if ButtonOn == true and hitp and GetYes() then
                            if _G.aimboton == true then
                                pcall(
                                    function()
                                        local g, onscreen =
                                            Work.CurrentCamera:WorldToScreenPoint(
                                            GetYes().Character:FindFirstChild(hitp).Position
                                        )
                                        local aimsss
                                        local Head = GetYes().Character.Head
                                        local Velocityx = Head.Velocity.X
                                        local Velocityy = Head.Velocity.Y
                                        local Velocityz = Head.Velocity.Z
                                        if localPlayer:DistanceFromCharacter(GetYes().Character.Head.Position) <= 20 then
                                            aimsss =
                                                Head.Position +
                                                Vector3.new(
                                                    (Velocityx >= 1 and 1) or (Velocityx <= -1 and -1) or 0,
                                                    (Velocityy >= 1 and 1) or (Velocityy <= -1 and -1) or 0,
                                                    (Velocityz >= 1 and 1) or (Velocityz <= -1 and -1) or 0
                                                )
                                        elseif localPlayer:DistanceFromCharacter(GetYes().Character.Head.Position) >= 20 then
                                            aimsss =
                                                Head.Position +
                                                Vector3.new(
                                                    (Velocityx >= 2.5 and 2.5) or (Velocityx <= -2.5 and -2.5) or 0,
                                                    (Velocityy >= 2.5 and 2.5) or (Velocityy <= -2.5 and -2.5) or 0,
                                                    (Velocityz >= 2.5 and 2.5) or (Velocityz <= -2.5 and -2.5) or 0
                                                )
                                        elseif localPlayer:DistanceFromCharacter(GetYes().Character.Head.Position) <= 40 then
                                            aimsss =
                                                Head.Position +
                                                Vector3.new(
                                                    (Velocityx >= 3 and 3) or (Velocityx <= -3 and -3) or 0,
                                                    (Velocityy >= 3 and 3) or (Velocityy <= -3 and -3) or 0,
                                                    (Velocityz >= 3 and 3) or (Velocityz <= -3 and -3) or 0
                                                )
                                        elseif localPlayer:DistanceFromCharacter(GetYes().Character.Head.Position) >= 40 then
                                            aimsss =
                                                Head.Position +
                                                Vector3.new(
                                                    (Velocityx >= 3.5 and 3.5) or (Velocityx <= -3.5 and -3.5) or 0,
                                                    (Velocityy >= 3.5 and 3.5) or (Velocityy <= -3.5 and -3.5) or 0,
                                                    (Velocityz >= 3.5 and 3.5) or (Velocityz <= -3.5 and -3.5) or 0
                                                )
                                        elseif localPlayer:DistanceFromCharacter(GetYes().Character.Head.Position) <= 60 then
                                            aimsss =
                                                Head.Position +
                                                Vector3.new(
                                                    (Velocityx >= 4 and 4) or (Velocityx <= -4 and -4) or 4,
                                                    (Velocityy >= 4 and 4) or (Velocityy <= -4 and -4) or 4,
                                                    (Velocityz >= 4 and 4) or (Velocityz <= -4 and -4) or 4
                                                )
                                        elseif localPlayer:DistanceFromCharacter(GetYes().Character.Head.Position) >= 60 then
                                            aimsss =
                                                Head.Position +
                                                Vector3.new(
                                                    (Velocityx >= 4.5 and 4.5) or (Velocityx <= -4.5 and -4.5) or 0,
                                                    (Velocityy >= 4.5 and 4.5) or (Velocityy <= -4.5 and -4.5) or 0,
                                                    (Velocityz >= 4.5 and 4.5) or (Velocityz <= -4.5 and -4.5) or 0
                                                )
                                        elseif localPlayer:DistanceFromCharacter(GetYes().Character.Head.Position) <= 80 then
                                            aimsss =
                                                Head.Position +
                                                Vector3.new(
                                                    (Velocityx >= 5 and 5) or (Velocityx <= -5 and -5) or 0,
                                                    (Velocityy >= 5 and 5) or (Velocityy <= -5 and -5) or 0,
                                                    (Velocityz >= 5 and 5) or (Velocityz <= -5 and -5) or 0
                                                )
                                        elseif localPlayer:DistanceFromCharacter(GetYes().Character.Head.Position) >= 80 then
                                            aimsss =
                                                Head.Position +
                                                Vector3.new(
                                                    (Velocityx >= 5.5 and 5.5) or (Velocityx <= -5.5 and -5.5) or 0,
                                                    (Velocityy >= 5.5 and 5.5) or (Velocityy <= -5.5 and -5.5) or 0,
                                                    (Velocityz >= 5.5 and 5.5) or (Velocityz <= -5.5 and -5.5) or 0
                                                )
                                        end
                                        if onscreen then
                                            return mousemoverel(
                                                Work.CurrentCamera:WorldToScreenPoint(aimsss).X -
                                                    Work.CurrentCamera:WorldToScreenPoint(mouse.Hit.p).X +
                                                    aimbotxpos,
                                                Work.CurrentCamera:WorldToScreenPoint(aimsss).Y -
                                                    Work.CurrentCamera:WorldToScreenPoint(mouse.Hit.p).Y +
                                                    aimbotypos
                                            )
                                        end
                                    end
                                )
                            end
                        end
                    end
                )
            end
        )

        spawn(
            function()
                repeat
                    wait()
                until allesp
                UpdtEsp("All")
            end
        )

        _G.EstoyTirando = false

        local ThrowKnifeggg = function()
            if _G.EstoyTirando == true then
                for i, v in pairs(Player.Backpack:GetChildren()) do
                    if v:IsA("Tool") and v.Name == "Knife" then
                        _G.EstoyTirando = false
                    end
                end
            else
                if CheckKnife() == true then
                    _G.EstoyTirando = true
                    for i = 1, 1 do
                        for i, v in pairs(Player.Backpack:GetChildren()) do
                            if v:IsA("Tool") and v.Name == "Knife" then
                                Player.Character.Humanoid:EquipTool(v)
                            end
                        end
                        if game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife then
                            local ElValorDelRotador =
                                game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.RotateValue.Value
                            if
                                game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                    "throwOrientation"
                                )
                             then
                                ElValorDelRotador =
                                    game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                    "throwOrientation"
                                ).Value
                            end
                            wait()
                            local Anim =
                                game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
                                game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.ThrowCharge
                            )
                            local nim =
                                game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
                                game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Throw
                            )
                            Anim:Play(0.1, 1, 2)
                            wait(Silent_AimSpeed)
                            Anim:Stop()
                            Update()
                            wait()
                            _G.on = true
                            local l__LocalPlayer__3 = game.Players.LocalPlayer
                            local l__mouse__6 = l__LocalPlayer__3:GetMouse()
                            local v24, v25 =
                                game.Workspace:FindPartOnRayWithIgnoreList(
                                Ray.new(l__mouse__6.UnitRay.Origin, l__mouse__6.UnitRay.Direction * 1000),
                                {l__LocalPlayer__3.Character, workspace.Pets, workspace.KnifeHost}
                            )
                            game.ReplicatedStorage.Remotes.ThrowKnife:FireServer(v25, 0.4, ElValorDelRotador)
                            nim:Play(0.1, 1, 3)
                            game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Handle.Swoosh.Pitch =
                                math.random(1100, 1300) / 1000
                            game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Handle.Swoosh:Play(

                            )
                            wait(.15)
                            Update()
                            wait()
                            _G.on = false
                            wait(.15)
                            Player.Character.Humanoid:UnequipTools()
                            wait(.30)
                        end
                    end
                    _G.EstoyTirando = false
                else
                    if game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife then
                        if _G.EstoyTirando == true then
                            return
                        else
                            _G.EstoyTirando = true
                            for i = 1, 1 do
                                if game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife then
                                    local ElValorDelRotador =
                                        game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.RotateValue.Value
                                    if
                                        game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                            "throwOrientation"
                                        )
                                     then
                                        ElValorDelRotador =
                                            game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                            "throwOrientation"
                                        ).Value
                                    end
                                    wait()
                                    local Anim =
                                        game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
                                        game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.ThrowCharge
                                    )
                                    local nim =
                                        game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
                                        game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Throw
                                    )
                                    Anim:Play(0.1, 1, 2)
                                    wait(Silent_AimSpeed)
                                    Anim:Stop()
                                    Update()
                                    wait()
                                    _G.on = true
                                    local l__LocalPlayer__3 = game.Players.LocalPlayer
                                    local l__mouse__6 = l__LocalPlayer__3:GetMouse()
                                    local v24, v25 =
                                        game.Workspace:FindPartOnRayWithIgnoreList(
                                        Ray.new(l__mouse__6.UnitRay.Origin, l__mouse__6.UnitRay.Direction * 1000),
                                        {l__LocalPlayer__3.Character, workspace.Pets, workspace.KnifeHost}
                                    )
                                    game.ReplicatedStorage.Remotes.ThrowKnife:FireServer(v25, 0.4, ElValorDelRotador)
                                    nim:Play(0.1, 1, 3)
                                    game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Handle.Swoosh.Pitch =
                                        math.random(1100, 1300) / 1000
                                    game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Handle.Swoosh:Play(

                                    )
                                    wait(.15)
                                    Update()
                                    wait()
                                    _G.on = false
                                    wait(.15)
                                    wait(.30)
                                end
                            end
                            _G.EstoyTirando = false
                        end
                    end
                end
            end
        end

        local getClosestPlayerToCursor = function()
            [nonamecall]
            local closestPlayer = nil
            local shortestDistance = math.huge

            for i, v in pairs(game:GetService("Players"):GetPlayers()) do
                if v.Name ~= localPlayer.Name then
                    if
                        v.Character and v.Character:FindFirstChild("Humanoid") and v.Character.Humanoid.Health ~= 0 and
                            v.Character:FindFirstChild("HumanoidRootPart") and
                            v.Character:FindFirstChild("Head")
                     then
                        local pos = currentCamera:WorldToViewportPoint(v.Character.HumanoidRootPart.Position)
                        local magnitude = (Vector2.new(pos.X, pos.Y) - Vector2.new(mouse.X, mouse.Y)).magnitude
                        local distance_from_plr =
                            (v.Character.HumanoidRootPart.Position - localPlayer.Character.HumanoidRootPart.Position).magnitude

                        if magnitude <= shortestDistance and distance_from_plr <= 700 then
                            closestPlayer = v
                            shortestDistance = magnitude
                        end
                    end
                end
            end
            return closestPlayer or localPlayer
        end

        local mt = getrawmetatable(game)
        local oldNamecall = mt.__namecall
        if setreadonly then
            setreadonly(mt, false)
        else
            make_writeable(mt, true)
        end
        local namecallMethod = getnamecallmethod or get_namecall_method
        local newClose = newcclosure or function(f)
                return f
            end

        mt.__namecall =
            newClose(
            function(...) [nonamecall]
                local method = namecallMethod()
                local args = {...}
                if _G.on then
                    if
                        tostring(method) == "FireServer" and tostring(args[1]) == "ThrowKnife" and
                            getClosestPlayerToCursor()
                     then
                        local Head = getClosestPlayerToCursor().Character.Head
                        local Velocityx = Head.Velocity.X
                        local Velocityy = Head.Velocity.Y
                        local Velocityz = Head.Velocity.Z
                        if localPlayer:DistanceFromCharacter(getClosestPlayerToCursor().Character.Head.Position) <= 20 then
                            args[2] =
                                Head.Position +
                                Vector3.new(
                                    (Velocityx >= 1 and 1) or (Velocityx <= -1 and -1) or 0,
                                    (Velocityy >= 1 and 1) or (Velocityy <= -1 and -1) or 0,
                                    (Velocityz >= 1 and 1) or (Velocityz <= -1 and -1) or 0
                                )
                        elseif
                            localPlayer:DistanceFromCharacter(getClosestPlayerToCursor().Character.Head.Position) >= 20
                         then
                            args[2] =
                                Head.Position +
                                Vector3.new(
                                    (Velocityx >= 2.5 and 2.5) or (Velocityx <= -2.5 and -2.5) or 0,
                                    (Velocityy >= 2.5 and 2.5) or (Velocityy <= -2.5 and -2.5) or 0,
                                    (Velocityz >= 2.5 and 2.5) or (Velocityz <= -2.5 and -2.5) or 0
                                )
                        elseif
                            localPlayer:DistanceFromCharacter(getClosestPlayerToCursor().Character.Head.Position) <= 40
                         then
                            args[2] =
                                Head.Position +
                                Vector3.new(
                                    (Velocityx >= 3 and 3) or (Velocityx <= -3 and -3) or 3,
                                    (Velocityy >= 3 and 3) or (Velocityy <= -3 and -3) or 3,
                                    (Velocityz >= 3 and 3) or (Velocityz <= -3 and -3) or 3
                                )
                        elseif
                            localPlayer:DistanceFromCharacter(getClosestPlayerToCursor().Character.Head.Position) >= 40
                         then
                            args[2] =
                                Head.Position +
                                Vector3.new(
                                    (Velocityx >= 3.5 and 3.5) or (Velocityx <= -3.5 and -3.5) or 3.5,
                                    (Velocityy >= 3.5 and 3.5) or (Velocityy <= -3.5 and -3.5) or 3.5,
                                    (Velocityz >= 3.5 and 3.5) or (Velocityz <= -3.5 and -3.5) or 3.5
                                )
                        elseif
                            localPlayer:DistanceFromCharacter(getClosestPlayerToCursor().Character.Head.Position) <= 60
                         then
                            args[2] =
                                Head.Position +
                                Vector3.new(
                                    (Velocityx >= 4 and 4) or (Velocityx <= -4 and -4) or 4,
                                    (Velocityy >= 4 and 4) or (Velocityy <= -4 and -4) or 4,
                                    (Velocityz >= 4 and 4) or (Velocityz <= -4 and -4) or 4
                                )
                        elseif
                            localPlayer:DistanceFromCharacter(getClosestPlayerToCursor().Character.Head.Position) >= 60
                         then
                            args[2] =
                                Head.Position +
                                Vector3.new(
                                    (Velocityx >= 4.5 and 4.5) or (Velocityx <= -4.5 and -4.5) or 4.5,
                                    (Velocityy >= 4.5 and 4.5) or (Velocityy <= -4.5 and -4.5) or 4.5,
                                    (Velocityz >= 4.5 and 4.5) or (Velocityz <= -4.5 and -4.5) or 4.5
                                )
                        elseif
                            localPlayer:DistanceFromCharacter(getClosestPlayerToCursor().Character.Head.Position) <= 80
                         then
                            args[2] =
                                Head.Position +
                                Vector3.new(
                                    (Velocityx >= 5 and 5) or (Velocityx <= -5 and -5) or 5,
                                    (Velocityy >= 5 and 5) or (Velocityy <= -5 and -5) or 5,
                                    (Velocityz >= 5 and 5) or (Velocityz <= -5 and -5) or 5
                                )
                        elseif
                            localPlayer:DistanceFromCharacter(getClosestPlayerToCursor().Character.Head.Position) >= 80
                         then
                            args[2] =
                                Head.Position +
                                Vector3.new(
                                    (Velocityx >= 5.5 and 5.5) or (Velocityx <= -5.5 and -5.5) or 5.5,
                                    (Velocityy >= 5.5 and 5.5) or (Velocityy <= -5.5 and -5.5) or 5.5,
                                    (Velocityz >= 5.5 and 5.5) or (Velocityz <= -5.5 and -5.5) or 5.5
                                )
                        end
                        args[3] = 0.4
                        args[4] =
                            game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.RotateValue.Value
                        if
                            game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                "throwOrientation"
                            )
                         then
                            args[4] =
                                game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                "throwOrientation"
                            ).Value
                        end
                        return oldNamecall(unpack(args))
                    end
                end
                return oldNamecall(...)
            end
        )
        if setreadonly then
            setreadonly(mt, true)
        else
            make_writeable(mt, false)
        end

        spawn(
            function()
                local Stab = function()
                    pcall(
                        function()
                            local DownStab
                            local StabPunch
                            local ThrowCharge
                            local Throw
                            local DualEquip

                            if game.Players.LocalPlayer.Character:FindFirstChild("UpperTorso") then
                                DownStab =
                                    game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
                                    game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                        "R15Animations"
                                    ).DownStab
                                )
                                StabPunch =
                                    game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
                                    game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                        "R15Animations"
                                    ).StabPunch
                                )
                                ThrowCharge =
                                    game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
                                    game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                        "R15Animations"
                                    ).ThrowCharge
                                )
                                Throw =
                                    game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
                                    game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                        "R15Animations"
                                    ).Throw
                                )
                                DualEquip =
                                    game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
                                    game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                        "R15Animations"
                                    ).DualEquip
                                )
                            else
                                DownStab =
                                    game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
                                    game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.DownStab
                                )
                                StabPunch =
                                    game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
                                    game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                        "StabPunch"
                                    )
                                )
                                ThrowCharge =
                                    game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
                                    game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                        "ThrowCharge"
                                    )
                                )
                                Throw =
                                    game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
                                    game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                        "Throw"
                                    )
                                )
                                DualEquip =
                                    game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(
                                    game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                        "DualEquip"
                                    )
                                )
                            end

                            local v15 = tick()
                            game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Handle.Swoosh.Pitch =
                                math.random(1100, 1300) / 1000
                            game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Handle.Swoosh:Play(

                            )

                            if
                                math.random(3) == 1 and
                                    not game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                        "noDownStab"
                                    )
                             then
                                local Gripxd =
                                    game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Grip
                                coroutine.wrap(
                                    function()
                                        game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Grip =
                                            Gripxd * CFrame.Angles(math.pi, 0, 0)
                                        wait(0.6)
                                        game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Grip =
                                            Gripxd
                                    end
                                )()
                                DownStab:Play(0.1, 1, 1.5)
                            else
                                StabPunch:Play(0.1, 1, 1.5)
                            end
                            game.Players.LocalPlayer.PlayerScripts.localknifehandler.HitCheck:Fire(
                                getClosestPlayer().Character
                            )
                        end
                    )
                end

                spawn(
                    function()
                        while wait() do
                            pcall(
                                function()
                                    if
                                        localPlayer:DistanceFromCharacter(
                                            getClosestPlayer().Character.HumanoidRootPart.Position
                                        ) <= 6 and
                                            getClosestPlayer().Name ~= localPlayer.Name and
                                            StabbingAssistance == true
                                     then
                                        spawn(
                                            function()
                                                pcall(
                                                    function()
                                                        if
                                                            game:GetService("Workspace")[
                                                                game:GetService("Players").LocalPlayer.Name
                                                            ].Knife.Parent.Parent == game:GetService("Workspace")
                                                         then
                                                            if StabbingAssistance == true then
                                                                local hv =
                                                                    math.floor(
                                                                    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position.Y -
                                                                        getClosestPlayer().Character.HumanoidRootPart.Position.Y
                                                                )
                                                                if hv <= 3 and hv >= -1 then --max 3 jump if its more than -3 and -11
                                                                    Stab()
                                                                elseif hv <= 3 and hv <= -1 then
                                                                    game.Players.LocalPlayer.Character.Humanoid.Jump =
                                                                        true
                                                                    wait(.55)
                                                                    Stab()
                                                                end
                                                            end
                                                        end
                                                    end
                                                )
                                            end
                                        )
                                        spawn(
                                            function()
                                                pcall(
                                                    function()
                                                        if CheckKnife() == true then
                                                            for i = 1, 1 do
                                                                for i, v in pairs(Player.Backpack:GetChildren()) do
                                                                    if v:IsA("Tool") and v.Name == "Knife" then
                                                                        Player.Character.Humanoid:EquipTool(v)
                                                                    end
                                                                end
                                                            end
                                                            wait()
                                                            if StabbingAssistance == true then
                                                                local hv =
                                                                    math.floor(
                                                                    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position.Y -
                                                                        getClosestPlayer().Character.HumanoidRootPart.Position.Y
                                                                )
                                                                if hv <= 3 and hv >= -1 then --max 3 jump if its more than -3 and -11
                                                                    Stab()
                                                                elseif hv <= 3 and hv <= -1 then
                                                                    game.Players.LocalPlayer.Character.Humanoid.Jump =
                                                                        true
                                                                    wait(.55)
                                                                    Stab()
                                                                end
                                                            end
                                                        end
                                                    end
                                                )
                                            end
                                        )
                                        wait(.50)
                                    end
                                end
                            )
                        end
                    end
                )
            end
        )
        local b = window:CreateFolder("Gui Settings")
        b:GuiSettings()

        local NewSilentAimBind = "r"
        local OldSilentAimBind = "f"

        ssilent:Box(
            "Silent Aim",
            "string",
            function(brrr)
                NewSilentAimBind = brrr
                wait()
            end
        )

        ssilent:Label("Default Key 'r', Not Spammable^", Color3.fromRGB(38, 38, 38), Color3.fromRGB(0, 216, 111))

        ssilent:Box(
            "Silent Aim",
            "string",
            function(brrr)
                wait()
                OldSilentAimBind = brrr
            end
        )
        ssilent:Label("Default Key 'f', Spammable^", Color3.fromRGB(38, 38, 38), Color3.fromRGB(0, 216, 111))

        Mouse.KeyDown:Connect(
            function(TheKeyPressed)
                if TheKeyPressed == (string.lower(NewSilentAimBind) or string.upper(NewSilentAimBind)) then
                    pcall(
                        function()
                            ThrowKnifeggg()
                        end
                    )
                end
            end
        )

        Mouse.KeyDown:Connect(
            function(TheKeyPressed)
                if TheKeyPressed == (string.lower(OldSilentAimBind) or string.upper(OldSilentAimBind)) then
                    pcall(
                        function()
                            for i, v in pairs(Player.Backpack:GetChildren()) do
                                if v:IsA("Tool") and v.Name == "Knife" then
                                    Player.Character.Humanoid:EquipTool(v)
                                end
                            end
                        end
                    )
                    spawn(
                        function()
                            pcall(
                                function()
                                    if game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife then
                                        local ElValorDelRotador =
                                            game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.RotateValue.Value
                                        if
                                            game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                                "throwOrientation"
                                            )
                                         then
                                            ElValorDelRotador =
                                                game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild(
                                                "throwOrientation"
                                            ).Value
                                        end
                                        spawn(
                                            function()
                                                pcall(
                                                    function()
                                                        wait()
